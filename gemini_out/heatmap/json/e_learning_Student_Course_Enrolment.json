{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "description": "Heatmap showing the count of courses enrolled by each student.",
    "data": {
        "url": "https://raw.githubusercontent.com/gzu-zxc/visual_coda/main/spider_csv/e_learning_Student_Course_Enrolment.csv"
    },
    "mark": {
        "type": "rect",
        "tooltip": true
    },
    "encoding": {
        "x": {
            "field": "student_id",
            "type": "nominal"
        },
        "y": {
            "field": "course_id",
            "type": "nominal"
        },
        "color": {
            "aggregate": "count",
            "type": "quantitative"
        }
    }
}