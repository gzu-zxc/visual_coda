{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "description": "A heatmap visualization showing the distribution of teachers by gender and address_id.",
    "data": {
        "url": "https://raw.githubusercontent.com/gzu-zxc/visual_coda/main/spider_csv/behavior_monitoring_Teachers.csv"
    },
    "mark": {
        "type": "rect",
        "tooltip": true
    },
    "encoding": {
        "x": {
            "field": "address_id",
            "type": "nominal"
        },
        "y": {
            "field": "gender",
            "type": "nominal"
        },
        "color": {
            "aggregate": "count",
            "type": "quantitative"
        }
    }
}