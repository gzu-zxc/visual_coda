{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "description": "Visualizes the relationship between channel market share and rating using a heatmap.",
    "data": {
        "url": "https://raw.githubusercontent.com/gzu-zxc/visual_coda/main/spider_csv/program_share_channel.csv"
    },
    "mark": {
        "type": "rect",
        "tooltip": true
    },
    "encoding": {
        "x": {
            "field": "Share_in_percent",
            "type": "quantitative",
            "bin": {
                "maxbins": 10
            }
        },
        "y": {
            "field": "Rating_in_percent",
            "type": "quantitative",
            "bin": {
                "maxbins": 10
            }
        },
        "color": {
            "aggregate": "count",
            "type": "quantitative"
        }
    }
}