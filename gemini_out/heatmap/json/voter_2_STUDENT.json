{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "description": "A heatmap showing the number of students in each city and major.",
    "data": {
        "url": "https://raw.githubusercontent.com/gzu-zxc/visual_coda/main/spider_csv/voter_2_STUDENT.csv"
    },
    "mark": {
        "type": "rect",
        "tooltip": true
    },
    "encoding": {
        "x": {
            "field": "city_code",
            "type": "nominal"
        },
        "y": {
            "field": "Major",
            "type": "nominal"
        },
        "color": {
            "aggregate": "count",
            "type": "quantitative"
        }
    }
}