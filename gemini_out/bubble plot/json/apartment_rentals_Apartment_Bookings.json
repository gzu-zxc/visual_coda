{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "description": "A bubble chart showing the relationship between booking start date and end date, sized by the apartment ID",
    "data": {
        "url": "https://raw.githubusercontent.com/gzu-zxc/visual_coda/main/spider_csv/apartment_rentals_Apartment_Bookings.csv"
    },
    "mark": {
        "type": "circle",
        "tooltip": true
    },
    "encoding": {
        "x": {
            "field": "booking_start_date",
            "type": "temporal",
            "timeUnit": "month"
        },
        "y": {
            "field": "booking_end_date",
            "type": "temporal",
            "timeUnit": "month"
        },
        "size": {
            "field": "apt_id",
            "type": "quantitative"
        }
    }
}