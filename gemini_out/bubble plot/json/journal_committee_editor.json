{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "description": "A bubble plot showing the relationship between age and editor ID, with the size of the bubbles representing the name.",
    "data": {
        "url": "https://raw.githubusercontent.com/gzu-zxc/visual_coda/main/spider_csv/journal_committee_editor.csv"
    },
    "mark": {
        "type": "circle",
        "tooltip": true
    },
    "encoding": {
        "x": {
            "field": "Age",
            "type": "quantitative"
        },
        "y": {
            "field": "Editor_ID",
            "type": "quantitative"
        },
        "size": {
            "field": "Name",
            "type": "nominal"
        }
    }
}