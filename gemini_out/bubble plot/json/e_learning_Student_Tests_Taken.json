{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "description": "A bubble plot showing the distribution of test results by date, with the size of the bubbles representing the number of students taking the test.",
    "data": {
        "url": "https://raw.githubusercontent.com/gzu-zxc/visual_coda/main/spider_csv/e_learning_Student_Tests_Taken.csv"
    },
    "mark": {
        "type": "circle",
        "tooltip": true
    },
    "encoding": {
        "x": {
            "field": "date_test_taken",
            "type": "temporal",
            "timeUnit": "day"
        },
        "y": {
            "field": "test_result",
            "type": "nominal"
        },
        "size": {
            "aggregate": "count",
            "field": "registration_id"
        }
    }
}