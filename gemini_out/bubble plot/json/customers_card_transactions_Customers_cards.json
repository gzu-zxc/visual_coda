{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "description": "A bubble plot showing the relationship between the date when the card becomes valid and the date when the card expires, with the size of the bubbles representing the card number.",
    "data": {
        "url": "https://raw.githubusercontent.com/gzu-zxc/visual_coda/main/spider_csv/customers_card_transactions_Customers_cards.csv"
    },
    "mark": {
        "type": "circle",
        "tooltip": true
    },
    "encoding": {
        "x": {
            "field": "date_valid_from",
            "type": "temporal",
            "timeUnit": "yearmonth"
        },
        "y": {
            "field": "date_valid_to",
            "type": "temporal",
            "timeUnit": "yearmonth"
        },
        "size": {
            "field": "card_number",
            "type": "quantitative"
        }
    }
}