{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "description": "A bubble plot showing the relationship between Votes and Vote_Percent, with the size of the bubbles representing Seats.",
    "data": {
        "url": "https://raw.githubusercontent.com/gzu-zxc/visual_coda/main/spider_csv/election_representative_election.csv"
    },
    "mark": {
        "type": "circle",
        "tooltip": true
    },
    "encoding": {
        "x": {
            "field": "Votes",
            "type": "quantitative"
        },
        "y": {
            "field": "Vote_Percent",
            "type": "quantitative"
        },
        "size": {
            "field": "Seats",
            "type": "quantitative"
        }
    }
}