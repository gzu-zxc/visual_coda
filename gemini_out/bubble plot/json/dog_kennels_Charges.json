{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "description": "A bubble chart showing the relationship between charge type and charge amount, with the size of the bubbles representing the number of charges.",
    "data": {
        "url": "https://raw.githubusercontent.com/gzu-zxc/visual_coda/main/spider_csv/dog_kennels_Charges.csv"
    },
    "mark": {
        "type": "circle",
        "tooltip": true
    },
    "encoding": {
        "x": {
            "field": "charge_type",
            "type": "nominal"
        },
        "y": {
            "field": "charge_amount",
            "type": "quantitative"
        },
        "size": {
            "field": "charge_id",
            "type": "quantitative",
            "aggregate": "count"
        }
    }
}