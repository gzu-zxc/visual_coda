{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "description": "A heatmap showing the maximum weight lifted in the snatch and clean and jerk exercises.",
    "data": {
        "url": "spider_csv/body_builder_body_builder.csv.csv"
    },
    "mark": {
        "type": "rect",
        "tooltip": true
    },
    "encoding": {
        "x": {
            "field": "Snatch",
            "type": "quantitative"
        },
        "y": {
            "field": "Clean_Jerk",
            "type": "quantitative"
        },
        "color": {
            "field": "Total",
            "type": "quantitative"
        }
    }
}