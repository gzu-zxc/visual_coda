{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "description": "A box plot chart showing the distribution of maximum gross weight of various aircraft.",
    "data": {
        "url": "spider_csv/aircraft_aircraft.csv.csv"
    },
    "mark": {
        "type": "boxplot",
        "tooltip": true
    },
    "encoding": {
        "y": {
            "field": "Max_Gross_Weight_num",
            "type": "quantitative"
        }
    },
    "transform": [
        {
            "calculate": "toNumber(datum.Max_Gross_Weight.replace(' lb (', ',').replace(' kg)', ''))",
            "as": "Max_Gross_Weight_num"
        }
    ]
}