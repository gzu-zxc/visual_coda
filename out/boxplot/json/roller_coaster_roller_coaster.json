{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "description": "A box plot showing the distribution of roller coaster Length, Height, and Speed.",
    "data": {
        "url": "spider_csv/roller_coaster_roller_coaster.csv.csv"
    },
    "mark": {
        "type": "boxplot",
        "tooltip": true
    },
    "encoding": {
        "x": {
            "field": "Length",
            "type": "quantitative"
        },
        "y": {
            "field": "Height",
            "type": "quantitative",
            "axis": {
                "title": "Height (feet)"
            }
        },
        "color": {
            "field": "Speed",
            "type": "quantitative",
            "scale": {
                "type": "linear"
            },
            "legend": {
                "title": "Speed (mph)"
            }
        }
    }
}