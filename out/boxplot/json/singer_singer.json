{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "description": "A Box Plot chart showing the distribution of net worth in millions by the birth year of the wealthiest singers.",
    "data": {
        "url": "spider_csv/singer_singer.csv.csv"
    },
    "mark": {
        "type": "boxplot",
        "tooltip": true
    },
    "encoding": {
        "x": {
            "field": "Birth_Year",
            "type": "temporal",
            "axis": {
                "title": "Birth Year"
            }
        },
        "y": {
            "field": "Net_Worth_Millions",
            "type": "quantitative",
            "axis": {
                "title": "Net Worth (Millions)"
            }
        }
    }
}