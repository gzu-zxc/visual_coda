{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "description": "A box plot chart showing the distribution of maximum aperture values by focal length of camera lenses used for mountain photography.",
    "data": {
        "url": "spider_csv/mountain_photos_camera_lens.csv.csv"
    },
    "mark": {
        "type": "boxplot",
        "tooltip": true
    },
    "encoding": {
        "x": {
            "field": "focal_length_mm",
            "type": "quantitative"
        },
        "y": {
            "field": "max_aperture",
            "type": "quantitative"
        }
    }
}