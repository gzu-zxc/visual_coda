{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "description": "A bubble plot showing the flight distance of different aircraft models.",
    "data": {
        "url": "spider_csv/flight_1_Aircraft.csv.csv"
    },
    "mark": {
        "type": "circle",
        "tooltip": true
    },
    "encoding": {
        "x": {
            "field": "name",
            "type": "nominal"
        },
        "y": {
            "field": "distance",
            "type": "quantitative"
        },
        "size": {
            "field": "aid",
            "type": "quantitative"
        }
    }
}