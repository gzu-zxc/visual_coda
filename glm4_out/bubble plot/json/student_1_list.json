{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "description": "A bubble plot showing the relationship between classroom number and grade level of students, with bubble size representing unique student IDs.",
    "data": {
        "url": "spider_csv/student_1_list.csv.csv"
    },
    "mark": {
        "type": "circle",
        "tooltip": true
    },
    "encoding": {
        "x": {
            "field": "Grade",
            "type": "ordinal"
        },
        "y": {
            "field": "Classroom",
            "type": "quantitative"
        },
        "size": {
            "field": "ID",
            "type": "quantitative"
        }
    }
}