{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "description": "A bubble plot showing employee age by city of residence.",
    "data": {
        "url": "spider_csv/employee_hire_evaluation_employee.csv.csv"
    },
    "mark": {
        "type": "circle",
        "tooltip": true
    },
    "encoding": {
        "size": {
            "field": "Age",
            "type": "quantitative"
        },
        "color": {
            "field": "City",
            "type": "nominal"
        }
    }
}