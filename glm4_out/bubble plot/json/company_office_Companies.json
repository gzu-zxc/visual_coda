{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "description": "A bubble plot showing the relationship between sales, profits, and market value of companies.",
    "data": {
        "url": "spider_csv/company_office_Companies.csv.csv"
    },
    "mark": {
        "type": "circle",
        "tooltip": true
    },
    "encoding": {
        "x": {
            "field": "Sales_billion",
            "type": "quantitative"
        },
        "y": {
            "field": "Profits_billion",
            "type": "quantitative"
        },
        "size": {
            "field": "Market_Value_billion",
            "type": "quantitative"
        }
    }
}