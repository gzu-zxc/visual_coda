{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "description": "A bubble plot showing the distribution of age, job, and city.",
    "data": {
        "url": "spider_csv/network_2_Person.csv.csv"
    },
    "mark": {
        "type": "circle",
        "tooltip": true
    },
    "encoding": {
        "x": {
            "field": "age",
            "type": "quantitative"
        },
        "y": {
            "field": "job",
            "type": "nominal"
        },
        "color": {
            "field": "city",
            "type": "nominal"
        }
    }
}