{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "description": "A bubble plot chart showing the match scores and results over time.",
    "data": {
        "url": "spider_csv/city_record_MATCH.csv.csv"
    },
    "mark": {
        "type": "circle",
        "tooltip": true
    },
    "encoding": {
        "x": {
            "field": "Date",
            "type": "temporal",
            "timeUnit": "month"
        },
        "y": {
            "field": "Score",
            "type": "ordinal"
        },
        "size": {
            "field": "Result",
            "type": "quantitative",
            "aggregate": "count"
        }
    }
}