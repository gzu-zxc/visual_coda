{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "description": "A bubble plot showing the height and prominence of various mountains.",
    "data": {
        "url": "spider_csv/climbing_mountain.csv.csv"
    },
    "mark": {
        "type": "circle",
        "tooltip": true
    },
    "encoding": {
        "x": {
            "field": "Prominence",
            "type": "quantitative"
        },
        "y": {
            "field": "Height",
            "type": "quantitative"
        },
        "size": {
            "field": "Height",
            "type": "quantitative",
            "scale": {
                "range": [
                    100,
                    1000
                ]
            }
        },
        "tooltip": [
            {
                "field": "Name",
                "type": "nominal"
            }
        ]
    }
}