{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "description": "A bubble plot showing product price vs product ID, colored by product type code.",
    "data": {
        "url": "spider_csv/customers_and_products_contacts_products.csv.csv"
    },
    "mark": {
        "type": "circle",
        "tooltip": true
    },
    "encoding": {
        "x": {
            "field": "product_id",
            "type": "quantitative"
        },
        "y": {
            "field": "product_price",
            "type": "quantitative"
        },
        "color": {
            "field": "product_type_code",
            "type": "nominal"
        },
        "size": {
            "field": "product_price",
            "type": "quantitative"
        }
    }
}