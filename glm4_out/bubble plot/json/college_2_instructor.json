{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "description": "Bubble plot of college instructor salaries by department.",
    "data": {
        "url": "spider_csv/college_2_instructor.csv.csv"
    },
    "mark": {
        "type": "circle",
        "tooltip": true
    },
    "encoding": {
        "x": {
            "field": "dept_name",
            "type": "nominal"
        },
        "y": {
            "field": "salary",
            "type": "quantitative"
        },
        "size": {
            "field": "ID",
            "type": "quantitative"
        }
    }
}