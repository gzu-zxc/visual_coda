{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "description": "A bubble plot showing the population of various counties.",
    "data": {
        "url": "spider_csv/election_county.csv.csv"
    },
    "mark": {
        "type": "circle",
        "tooltip": true
    },
    "encoding": {
        "size": {
            "field": "Population",
            "type": "quantitative"
        },
        "text": {
            "field": "County_name",
            "type": "nominal"
        }
    }
}