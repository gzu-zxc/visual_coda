{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "description": "Bubble plot chart showing the launched year, speed of ships, and mission identifier.",
    "data": {
        "url": "spider_csv/ship_mission_mission.csv.csv"
    },
    "mark": {
        "type": "circle",
        "tooltip": true
    },
    "encoding": {
        "x": {
            "field": "Launched_Year",
            "type": "temporal",
            "timeUnit": "year"
        },
        "y": {
            "field": "Speed_knots",
            "type": "quantitative"
        },
        "size": {
            "field": "Mission_ID",
            "type": "quantitative"
        }
    }
}