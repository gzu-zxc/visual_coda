{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "description": "A multi-series colored line chart showing the number of bookings per apartment over time.",
    "data": {
        "url": "spider_csv/apartment_rentals_Apartment_Bookings.csv"
    },
    "mark": {
        "type": "line",
        "tooltip": true
    },
    "encoding": {
        "x": {
            "field": "booking_start_date",
            "type": "temporal",
            "timeUnit": "month"
        },
        "y": {
            "field": "count",
            "type": "quantitative"
        },
        "color": {
            "field": "apt_id",
            "type": "nominal"
        }
    },
    "transform": [
        {
            "aggregate": [
                {
                    "op": "count",
                    "field": "apt_booking_id",
                    "as": "count"
                }
            ],
            "groupby": [
                "apt_id",
                "booking_start_date"
            ]
        }
    ]
}