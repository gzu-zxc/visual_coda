{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "description": "A multi-series colored line chart showing insurance policy durations by type.",
    "data": {
        "url": "spider_csv/insurance_and_eClaims_policies.csv"
    },
    "mark": {
        "type": "line",
        "tooltip": true
    },
    "encoding": {
        "x": {
            "field": "Start_Date",
            "type": "temporal",
            "timeUnit": "yearmonthdate"
        },
        "y": {
            "field": "End_Date",
            "type": "temporal",
            "timeUnit": "yearmonthdate"
        },
        "color": {
            "field": "Policy_Type_Code",
            "type": "nominal"
        }
    },
    "transform": [
        {
            "calculate": "datum.End_Date - datum.Start_Date",
            "as": "Policy_Duration"
        }
    ]
}