{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "description": "A multi-series colored line chart showing the relationship between the number of final tables made, total earnings, and best finish position of poker players.",
    "data": {
        "url": "spider_csv/poker_player_poker_player.csv"
    },
    "mark": {
        "type": "line",
        "tooltip": true
    },
    "encoding": {
        "x": {
            "field": "Final_Table_Made",
            "type": "quantitative"
        },
        "y": {
            "field": "Earnings",
            "type": "quantitative"
        },
        "color": {
            "field": "Best_Finish",
            "type": "ordinal"
        }
    }
}