{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "description": "A multi-series colored line chart showing the maximum weight lifted in Snatch and Clean Jerk by body builders.",
    "data": {
        "url": "spider_csv/body_builder_body_builder.csv"
    },
    "mark": {
        "type": "line",
        "tooltip": true
    },
    "encoding": {
        "x": {
            "field": "Snatch",
            "type": "quantitative"
        },
        "y": {
            "field": "Clean_Jerk",
            "type": "quantitative"
        },
        "color": {
            "field": "Body_Builder_ID",
            "type": "nominal"
        }
    }
}