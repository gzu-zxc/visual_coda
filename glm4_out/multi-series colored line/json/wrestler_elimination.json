{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "description": "A multi-series colored line chart showing the number of wrestler eliminations over time for each team.",
    "data": {
        "url": "spider_csv/wrestler_elimination.csv"
    },
    "mark": {
        "type": "line",
        "tooltip": true
    },
    "encoding": {
        "x": {
            "field": "Time",
            "type": "temporal",
            "timeUnit": "minute"
        },
        "y": {
            "field": "Wrestler_ID",
            "type": "quantitative",
            "aggregate": "count"
        },
        "color": {
            "field": "Team",
            "type": "nominal"
        }
    },
    "groupby": [
        "Team"
    ]
}