{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "description": "A multi-series colored line chart showing the net worth of singers by birth year and citizenship.",
    "data": {
        "url": "spider_csv/singer_singer.csv"
    },
    "mark": {
        "type": "line",
        "tooltip": true
    },
    "encoding": {
        "x": {
            "field": "Birth_Year",
            "type": "temporal",
            "timeUnit": "year"
        },
        "y": {
            "field": "Net_Worth_Millions",
            "type": "quantitative",
            "aggregate": "mean"
        },
        "color": {
            "field": "Citizenship",
            "type": "nominal"
        }
    },
    "transform": [
        {
            "aggregate": [
                {
                    "op": "mean",
                    "field": "Net_Worth_Millions",
                    "as": "Net_Worth_Millions"
                }
            ],
            "groupby": [
                "Citizenship",
                "Birth_Year"
            ]
        }
    ],
    "sort": {
        "field": "Birth_Year"
    }
}