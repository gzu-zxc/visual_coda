{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "description": "A multi-series colored line chart showing the age distribution across different majors.",
    "data": {
        "url": "spider_csv/allergy_1_Student.csv"
    },
    "mark": {
        "type": "line",
        "tooltip": true
    },
    "encoding": {
        "x": {
            "field": "Age",
            "type": "quantitative"
        },
        "color": {
            "field": "Major",
            "type": "ordinal"
        }
    }
}