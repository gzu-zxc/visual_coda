{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "description": "A multi-series colored line chart showing the age of museum visitors against their level of membership.",
    "data": {
        "url": "spider_csv/museum_visit_visitor.csv"
    },
    "mark": {
        "type": "line",
        "tooltip": true
    },
    "encoding": {
        "x": {
            "field": "Level_of_membership",
            "type": "ordinal"
        },
        "y": {
            "field": "Age",
            "type": "quantitative"
        },
        "color": {
            "field": "Name",
            "type": "nominal"
        }
    }
}