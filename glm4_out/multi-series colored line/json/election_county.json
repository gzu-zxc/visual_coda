{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "description": "A multi-series colored line chart showing population by county.",
    "data": {
        "url": "spider_csv/election_county.csv"
    },
    "mark": {
        "type": "line",
        "tooltip": true
    },
    "encoding": {
        "x": {
            "field": "County_name",
            "type": "nominal",
            "sort": {
                "field": "Population",
                "order": "descending"
            }
        },
        "y": {
            "field": "Population",
            "type": "quantitative"
        },
        "color": {
            "field": "Zip_code",
            "type": "nominal"
        }
    }
}