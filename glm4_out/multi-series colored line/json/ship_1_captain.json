{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "description": "A multi-series colored line chart showing the age distribution of ship captains by rank.",
    "data": {
        "url": "spider_csv/ship_1_captain.csv"
    },
    "mark": {
        "type": "line",
        "tooltip": true
    },
    "encoding": {
        "x": {
            "field": "age",
            "type": "quantitative"
        },
        "y": {
            "field": "Captain_ID",
            "type": "nominal"
        },
        "color": {
            "field": "Rank",
            "type": "ordinal"
        }
    },
    "sort": {
        "field": "age"
    }
}