{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "description": "A multi-series colored line chart showing the height and weight of candidates over time.",
    "data": {
        "url": "spider_csv/candidate_poll_people.csv"
    },
    "mark": {
        "type": "line",
        "tooltip": true
    },
    "encoding": {
        "x": {
            "field": "Date_of_Birth",
            "type": "temporal",
            "timeUnit": "year"
        },
        "y": {
            "field": "Height",
            "type": "quantitative"
        },
        "color": {
            "field": "Sex",
            "type": "nominal"
        }
    }
}