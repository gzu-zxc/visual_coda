{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "description": "A multi-series colored line chart showing the charge amounts for different charge types at a dog kennel.",
    "data": {
        "url": "spider_csv/dog_kennels_Charges.csv"
    },
    "mark": {
        "type": "line",
        "tooltip": true
    },
    "encoding": {
        "x": {
            "field": "charge_type",
            "type": "nominal"
        },
        "y": {
            "field": "charge_amount",
            "type": "quantitative"
        },
        "color": {
            "field": "charge_type",
            "type": "nominal"
        }
    },
    "sort": {
        "field": "charge_type"
    }
}