{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "description": "A multi-series colored line chart showing the sum of employer_organisation_id by staff_id and staff_details.",
    "data": {
        "url": "spider_csv/tracking_grants_for_research_Research_Staff.csv"
    },
    "mark": {
        "type": "line",
        "tooltip": true
    },
    "encoding": {
        "x": {
            "field": "staff_id",
            "type": "ordinal"
        },
        "y": {
            "field": "sum_employer_organisation_id",
            "type": "quantitative"
        },
        "color": {
            "field": "staff_details",
            "type": "nominal"
        }
    },
    "transform": [
        {
            "aggregate": [
                {
                    "op": "sum",
                    "field": "employer_organisation_id",
                    "as": "sum_employer_organisation_id"
                }
            ],
            "groupby": [
                "staff_id",
                "staff_details"
            ]
        }
    ]
}