{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "description": "Box plot of ticket prices over the years.",
    "data": {
        "url": "spider_csv/theme_gallery_exhibition.csv.csv"
    },
    "mark": {
        "type": "boxplot",
        "tooltip": true
    },
    "encoding": {
        "x": {
            "field": "Year",
            "type": "temporal",
            "timeUnit": "year"
        },
        "y": {
            "field": "Ticket_Price",
            "type": "quantitative"
        }
    }
}