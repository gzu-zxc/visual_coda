{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "description": "A box plot chart showing the distribution of wedding church unique identifiers by the year they were opened.",
    "data": {
        "url": "spider_csv/wedding_Church.csv.csv"
    },
    "mark": {
        "type": "boxplot",
        "tooltip": true
    },
    "encoding": {
        "x": {
            "field": "Open_Date",
            "type": "temporal",
            "timeUnit": "year"
        },
        "y": {
            "field": "Church_ID",
            "type": "quantitative"
        }
    }
}