{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "description": "A box plot showing the distribution of the number of employees across different departments.",
    "data": {
        "url": "spider_csv/college_1_department.csv.csv"
    },
    "mark": {
        "type": "boxplot",
        "tooltip": true
    },
    "encoding": {
        "x": {
            "field": "DEPT_NAME",
            "type": "nominal"
        },
        "y": {
            "field": "EMP_NUM",
            "type": "quantitative"
        }
    }
}