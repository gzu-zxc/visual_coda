{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "description": "A box plot chart showing the distribution of address identifiers for each teacher.",
    "data": {
        "url": "spider_csv/behavior_monitoring_Teachers.csv.csv"
    },
    "mark": {
        "type": "boxplot",
        "tooltip": true
    },
    "encoding": {
        "x": {
            "field": "teacher_id",
            "type": "nominal"
        },
        "y": {
            "field": "address_id",
            "type": "quantitative"
        }
    }
}