{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "description": "A box plot chart showing the distribution of height and weight of participants in a candidate poll.",
    "data": {
        "url": "spider_csv/candidate_poll_people.csv.csv"
    },
    "mark": {
        "type": "box-plot",
        "tooltip": true
    },
    "encoding": {
        "x": {
            "field": "Height",
            "type": "quantitative"
        },
        "y": {
            "field": "Weight",
            "type": "quantitative"
        }
    }
}