{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "description": "A box plot chart showing the distribution of workshops over time.",
    "data": {
        "url": "spider_csv/workshop_paper_workshop.csv.csv"
    },
    "mark": {
        "type": "boxplot",
        "tooltip": true
    },
    "encoding": {
        "x": {
            "field": "Workshop_ID",
            "type": "nominal"
        },
        "facet": {
            "field": "Date",
            "type": "ordinal",
            "timeUnit": "yearmonthdate"
        }
    }
}