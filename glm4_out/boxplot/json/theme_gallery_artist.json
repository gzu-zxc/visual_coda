{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "description": "Box plot chart showing the distribution of artists' ages over the years they joined the gallery.",
    "data": {
        "url": "spider_csv/theme_gallery_artist.csv.csv"
    },
    "mark": {
        "type": "boxplot",
        "tooltip": true
    },
    "encoding": {
        "x": {
            "field": "Year_Join",
            "type": "temporal",
            "timeUnit": "year"
        },
        "y": {
            "field": "Age",
            "type": "quantitative"
        }
    }
}