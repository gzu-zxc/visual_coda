{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "description": "A box plot chart showing the distribution of battle IDs by the outcome of the battles.",
    "data": {
        "url": "spider_csv/battle_death_battle.csv.csv"
    },
    "mark": {
        "type": "boxplot",
        "tooltip": true
    },
    "encoding": {
        "x": {
            "field": "result",
            "type": "ordinal"
        },
        "y": {
            "field": "id",
            "type": "quantitative"
        }
    }
}