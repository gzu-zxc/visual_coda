{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "description": "Box plot of school bus driver ages categorized by political party affiliation.",
    "data": {
        "url": "spider_csv/school_bus_driver.csv.csv"
    },
    "mark": {
        "type": "boxplot",
        "tooltip": true
    },
    "encoding": {
        "x": {
            "field": "Party",
            "type": "nominal"
        },
        "y": {
            "field": "Age",
            "type": "quantitative",
            "scale": {
                "zero": false
            }
        }
    }
}