{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "description": "A box plot chart showing the distribution of wine appellations by area.",
    "data": {
        "url": "spider_csv/wine_1_APPELLATIONS.csv.csv"
    },
    "mark": {
        "type": "box",
        "tooltip": true
    },
    "encoding": {
        "x": {
            "field": "Appelation",
            "type": "nominal"
        },
        "color": {
            "field": "Area",
            "type": "nominal"
        }
    }
}