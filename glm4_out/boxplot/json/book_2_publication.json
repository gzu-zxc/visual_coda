{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "description": "A box plot chart showing the distribution of book prices.",
    "data": {
        "url": "spider_csv/book_2_publication.csv.csv"
    },
    "mark": {
        "type": "boxplot",
        "tooltip": true
    },
    "encoding": {
        "x": {
            "field": "Publisher",
            "type": "nominal"
        },
        "y": {
            "field": "Price",
            "type": "quantitative",
            "scale": {
                "zero": false
            }
        }
    }
}