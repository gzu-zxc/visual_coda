{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "description": "A box plot chart showing the distribution of low and high estimates for different film types.",
    "data": {
        "url": "spider_csv/film_rank_film_market_estimation.csv.csv"
    },
    "mark": {
        "type": "box-plot",
        "tooltip": true
    },
    "encoding": {
        "x": {
            "field": "Type",
            "type": "nominal"
        },
        "y": [
            {
                "field": "Low_Estimate",
                "type": "quantitative"
            },
            {
                "field": "High_Estimate",
                "type": "quantitative"
            }
        ]
    }
}