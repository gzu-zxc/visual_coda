{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "description": "A box plot chart showing the distribution of payment amounts by payment method.",
    "data": {
        "url": "spider_csv/insurance_policies_Payments.csv.csv"
    },
    "mark": {
        "type": "boxplot",
        "tooltip": true
    },
    "encoding": {
        "x": {
            "field": "Payment_Method_Code",
            "type": "nominal"
        },
        "y": {
            "field": "Amount_Payment",
            "type": "quantitative"
        }
    }
}