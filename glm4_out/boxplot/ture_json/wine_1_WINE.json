{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "description": "Box plot of wine scores by grape variety.",
    "data": {
        "url": "spider_csv/wine_1_WINE.csv.csv"
    },
    "mark": {
        "type": "boxplot",
        "tooltip": true
    },
    "encoding": {
        "x": {
            "field": "Grape",
            "type": "nominal"
        },
        "y": {
            "field": "Score",
            "type": "quantitative"
        }
    }
}