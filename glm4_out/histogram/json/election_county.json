{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "description": "A histogram showing the distribution of county populations.",
    "data": {
        "url": "spider_csv/election_county.csv.csv"
    },
    "mark": {
        "type": "bar",
        "tooltip": true
    },
    "encoding": {
        "x": {
            "field": "Population",
            "type": "quantitative",
            "bin": true,
            "scale": {
                "zero": false
            }
        },
        "y": {
            "aggregate": "count",
            "type": "quantitative"
        }
    },
    "config": {
        "axis": {
            "labelFontSize": 12,
            "titleFontSize": 14
        }
    }
}