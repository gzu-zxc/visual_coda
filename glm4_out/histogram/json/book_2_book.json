{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "description": "A histogram showing the distribution of the number of issues per book.",
    "data": {
        "url": "spider_csv/book_2_book.csv.csv"
    },
    "mark": "bar",
    "encoding": {
        "x": {
            "field": "Issues",
            "type": "quantitative",
            "bin": true
        },
        "y": {
            "aggregate": "count",
            "type": "quantitative"
        }
    }
}