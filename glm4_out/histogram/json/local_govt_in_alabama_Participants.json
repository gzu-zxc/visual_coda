{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "description": "A histogram showing the distribution of Participant_IDs in local government events in Alabama.",
    "data": {
        "url": "spider_csv/local_govt_in_alabama_Participants.csv.csv"
    },
    "mark": {
        "type": "bar",
        "tooltip": true
    },
    "encoding": {
        "x": {
            "field": "Participant_ID",
            "type": "quantitative",
            "bin": true
        },
        "y": {
            "aggregate": "count",
            "type": "quantitative"
        }
    }
}