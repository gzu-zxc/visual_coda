{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "description": "A histogram showing the distribution of base prices for rooms at the inn.",
    "data": {
        "url": "spider_csv/inn_1_Rooms.csv.csv"
    },
    "mark": {
        "type": "bar",
        "tooltip": true
    },
    "encoding": {
        "x": {
            "field": "basePrice",
            "type": "quantitative",
            "bin": true
        },
        "y": {
            "type": "quantitative",
            "aggregate": "count"
        }
    }
}