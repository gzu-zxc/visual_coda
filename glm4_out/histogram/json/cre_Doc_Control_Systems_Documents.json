{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "description": "A histogram showing the distribution of receipt numbers for documents in the system.",
    "data": {
        "url": "spider_csv/cre_Doc_Control_Systems_Documents.csv.csv"
    },
    "mark": {
        "type": "bar",
        "tooltip": true
    },
    "encoding": {
        "x": {
            "field": "receipt_number",
            "type": "quantitative",
            "bin": true
        },
        "y": {
            "type": "quantitative",
            "aggregate": "count"
        }
    }
}