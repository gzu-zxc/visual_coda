{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "description": "A histogram showing the distribution of races over time.",
    "data": {
        "url": "spider_csv/race_track_race.csv.csv"
    },
    "mark": {
        "type": "bar",
        "tooltip": true
    },
    "encoding": {
        "x": {
            "field": "Date",
            "type": "temporal",
            "bin": {
                "maxbins": 20
            }
        },
        "y": {
            "aggregate": "count",
            "type": "quantitative"
        }
    }
}