{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "description": "A histogram showing the distribution of playlist unique identifiers across different categories.",
    "data": {
        "url": "spider_csv/store_1_playlists.csv.csv"
    },
    "mark": {
        "type": "bar",
        "tooltip": true
    },
    "encoding": {
        "x": {
            "field": "id",
            "type": "quantitative",
            "bin": true
        },
        "color": {
            "field": "name",
            "type": "nominal"
        }
    }
}