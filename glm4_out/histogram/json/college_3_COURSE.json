{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "description": "A histogram showing the distribution of course credits.",
    "data": {
        "url": "spider_csv/college_3_COURSE.csv.csv"
    },
    "mark": {
        "type": "bar",
        "tooltip": true
    },
    "encoding": {
        "x": {
            "field": "Credits",
            "type": "ordinal"
        },
        "y": {
            "aggregate": "count",
            "type": "quantitative"
        }
    }
}