{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "description": "A histogram showing the distribution of journalist ages.",
    "data": {
        "url": "spider_csv/news_report_journalist.csv.csv"
    },
    "mark": {
        "type": "bar",
        "tooltip": true
    },
    "encoding": {
        "x": {
            "field": "Age",
            "type": "quantitative",
            "bin": true
        },
        "y": {
            "field": "journalist_ID",
            "type": "quantitative",
            "aggregate": "count"
        }
    }
}