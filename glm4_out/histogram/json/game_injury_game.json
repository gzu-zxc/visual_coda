{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "description": "A histogram showing the number of game injuries per year.",
    "data": {
        "url": "spider_csv/game_injury_game.csv.csv"
    },
    "mark": {
        "type": "bar",
        "tooltip": true
    },
    "encoding": {
        "x": {
            "field": "Date",
            "type": "temporal",
            "timeUnit": "year",
            "axis": {
                "title": "Year"
            }
        },
        "y": {
            "aggregate": "count",
            "axis": {
                "title": "Number of Injuries"
            }
        }
    }
}