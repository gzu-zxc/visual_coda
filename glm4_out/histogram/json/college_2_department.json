{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "description": "A histogram showing the distribution of department budgets in a college.",
    "data": {
        "url": "spider_csv/college_2_department.csv.csv"
    },
    "mark": {
        "type": "bar",
        "tooltip": true
    },
    "encoding": {
        "x": {
            "field": "budget",
            "type": "quantitative",
            "bin": {
                "maxbins": 50
            }
        },
        "y": {
            "type": "quantitative",
            "aggregate": "count"
        }
    }
}