{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "description": "A histogram showing the number of workshops per month.",
    "data": {
        "url": "spider_csv/workshop_paper_workshop.csv.csv"
    },
    "mark": {
        "type": "bar",
        "tooltip": true
    },
    "encoding": {
        "x": {
            "field": "Date",
            "type": "temporal",
            "timeUnit": "month",
            "axis": {
                "title": "Month",
                "labelAngle": 0
            }
        },
        "y": {
            "aggregate": "count",
            "axis": {
                "title": "Number of Workshops"
            }
        }
    }
}