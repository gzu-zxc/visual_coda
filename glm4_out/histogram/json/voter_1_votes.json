{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "description": "A histogram showing the frequency of votes cast over time.",
    "data": {
        "url": "spider_csv/voter_1_votes.csv.csv"
    },
    "mark": {
        "type": "bar",
        "tooltip": true
    },
    "encoding": {
        "x": {
            "field": "created",
            "type": "temporal",
            "timeUnit": "interval",
            "axis": {
                "title": "Time of Vote"
            }
        },
        "y": {
            "aggregate": "count",
            "axis": {
                "title": "Number of Votes"
            }
        }
    }
}