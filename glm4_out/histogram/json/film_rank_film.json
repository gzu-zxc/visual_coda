{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "description": "A histogram showing the distribution of gross earnings in dollars for movies.",
    "data": {
        "url": "spider_csv/film_rank_film.csv.csv"
    },
    "mark": {
        "type": "bar",
        "tooltip": true
    },
    "encoding": {
        "x": {
            "field": "Gross_in_dollar",
            "type": "quantitative",
            "bin": true
        },
        "y": {
            "aggregate": "count",
            "type": "quantitative"
        }
    }
}