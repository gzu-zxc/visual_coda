{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "description": "A histogram showing the distribution of ministers taking office over the years.",
    "data": {
        "url": "spider_csv/party_people_party.csv.csv"
    },
    "mark": {
        "type": "bar",
        "tooltip": true
    },
    "encoding": {
        "x": {
            "field": "Took_office",
            "type": "temporal",
            "timeUnit": "year"
        },
        "y": {
            "aggregate": "count",
            "type": "quantitative"
        }
    }
}