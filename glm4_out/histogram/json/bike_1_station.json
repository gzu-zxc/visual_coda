{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "description": "A histogram showing the distribution of dock counts across bike share stations.",
    "data": {
        "url": "spider_csv/bike_1_station.csv.csv"
    },
    "mark": {
        "type": "bar",
        "tooltip": true
    },
    "encoding": {
        "x": {
            "field": "dock_count",
            "type": "ordinal"
        },
        "y": {
            "aggregate": "count",
            "type": "quantitative"
        }
    }
}