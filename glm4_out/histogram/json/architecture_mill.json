{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "description": "A histogram showing the distribution of mill construction years.",
    "data": {
        "url": "spider_csv/architecture_mill.csv.csv"
    },
    "mark": {
        "type": "bar",
        "tooltip": true
    },
    "encoding": {
        "x": {
            "field": "built_year",
            "type": "quantitative",
            "bin": {
                "maxbins": 20
            }
        },
        "y": {
            "type": "quantitative",
            "aggregate": "count"
        }
    }
}