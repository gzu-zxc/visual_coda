{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "description": "A histogram showing the frequency of votes received by players in the Baseball Hall of Fame voting results.",
    "data": {
        "url": "spider_csv/baseball_1_hall_of_fame.csv.csv"
    },
    "mark": {
        "type": "bar",
        "tooltip": true
    },
    "encoding": {
        "x": {
            "field": "votes",
            "type": "quantitative",
            "bin": true
        },
        "y": {
            "field": "count",
            "type": "quantitative"
        }
    }
}