{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "description": "A histogram showing the distribution of account balances.",
    "data": {
        "url": "spider_csv/loan_1_customer.csv.csv"
    },
    "mark": {
        "type": "bar",
        "tooltip": true
    },
    "encoding": {
        "x": {
            "field": "acc_bal",
            "type": "quantitative",
            "bin": true
        },
        "y": {
            "field": "count",
            "type": "quantitative"
        }
    }
}