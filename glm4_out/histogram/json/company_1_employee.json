{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "description": "A histogram showing the distribution of employee salaries at Company 1.",
    "data": {
        "url": "spider_csv/company_1_employee.csv.csv"
    },
    "mark": {
        "type": "bar",
        "tooltip": true
    },
    "encoding": {
        "x": {
            "field": "Salary",
            "type": "quantitative",
            "bin": true
        },
        "y": {
            "type": "quantitative",
            "aggregate": "count"
        }
    }
}