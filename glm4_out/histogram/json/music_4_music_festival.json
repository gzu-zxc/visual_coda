{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "description": "A histogram showing the distribution of award volumes across different categories.",
    "data": {
        "url": "spider_csv/music_4_music_festival.csv.csv"
    },
    "mark": {
        "type": "bar",
        "tooltip": true
    },
    "encoding": {
        "x": {
            "field": "Volume",
            "type": "quantitative",
            "bin": true
        },
        "color": {
            "field": "Category",
            "type": "nominal"
        }
    }
}