{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "description": "A histogram showing the distribution of railway managers' ages across different management levels.",
    "data": {
        "url": "spider_csv/railway_manager.csv.csv"
    },
    "mark": {
        "type": "bar",
        "tooltip": true
    },
    "encoding": {
        "x": {
            "field": "Age",
            "type": "quantitative",
            "bin": true
        },
        "color": {
            "field": "Level",
            "type": "ordinal"
        }
    }
}