{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "description": "A histogram showing the number of customer events by month.",
    "data": {
        "url": "spider_csv/local_govt_and_lot_customer_events.csv.csv"
    },
    "mark": {
        "type": "bar",
        "tooltip": true
    },
    "encoding": {
        "x": {
            "field": "date_moved_in",
            "type": "temporal",
            "timeUnit": "month",
            "axis": {
                "title": "Month Moved In"
            }
        },
        "y": {
            "aggregate": "count",
            "type": "quantitative",
            "axis": {
                "title": "Number of Events"
            }
        }
    }
}