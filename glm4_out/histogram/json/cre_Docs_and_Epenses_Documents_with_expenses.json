{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "description": "A histogram showing the distribution of Document_IDs.",
    "data": {
        "url": "spider_csv/cre_Docs_and_Epenses_Documents_with_expenses.csv.csv"
    },
    "mark": {
        "type": "bar",
        "tooltip": true
    },
    "encoding": {
        "x": {
            "field": "Document_ID",
            "type": "quantitative",
            "bin": true
        },
        "y": {
            "aggregate": "count",
            "field": "Document_ID",
            "type": "quantitative"
        }
    }
}