{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "description": "A histogram showing the distribution of student GPA scores at College 1.",
    "data": {
        "url": "spider_csv/college_1_student.csv.csv"
    },
    "mark": {
        "type": "bar",
        "tooltip": true
    },
    "encoding": {
        "x": {
            "field": "STU_GPA",
            "type": "quantitative",
            "bin": {
                "maxbins": 30
            }
        },
        "y": {
            "field": "count",
            "type": "quantitative"
        }
    }
}