{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "description": "A histogram showing the distribution of student grades.",
    "data": {
        "url": "spider_csv/student_1_list.csv.csv"
    },
    "mark": {
        "type": "bar",
        "tooltip": true
    },
    "encoding": {
        "x": {
            "field": "Grade",
            "type": "ordinal",
            "bin": true
        },
        "y": {
            "aggregate": "count",
            "type": "quantitative"
        }
    }
}