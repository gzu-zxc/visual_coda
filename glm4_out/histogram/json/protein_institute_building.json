{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "description": "A histogram showing the distribution of building heights in feet at the Protein Institute.",
    "data": {
        "url": "spider_csv/protein_institute_building.csv.csv"
    },
    "mark": "bar",
    "encoding": {
        "x": {
            "field": "Height_feet",
            "type": "quantitative",
            "bin": true
        },
        "y": {
            "aggregate": "count",
            "field": "Height_feet",
            "type": "quantitative"
        }
    }
}