{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "description": "A histogram showing the distribution of insurance policy start dates by month or year.",
    "data": {
        "url": "spider_csv/insurance_and_eClaims_policies.csv.csv"
    },
    "mark": {
        "type": "bar",
        "tooltip": true
    },
    "encoding": {
        "x": {
            "field": "Start_Date",
            "type": "temporal",
            "timeUnit": "yearmonth",
            "axis": {
                "title": "Policy Start Date (Month-Year)"
            }
        },
        "y": {
            "aggregate": "count",
            "type": "quantitative",
            "axis": {
                "title": "Number of Policies"
            }
        }
    }
}