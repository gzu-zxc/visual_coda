{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "description": "A histogram showing the distribution of trip durations in seconds.",
    "data": {
        "url": "spider_csv/bike_1_trip.csv.csv"
    },
    "mark": {
        "type": "bar",
        "tooltip": true
    },
    "encoding": {
        "x": {
            "field": "duration",
            "type": "quantitative",
            "bin": true
        },
        "y": {
            "field": "count",
            "type": "quantitative"
        }
    }
}