{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "description": "A histogram showing the distribution of final tables made by poker players.",
    "data": {
        "url": "spider_csv/poker_player_poker_player.csv.csv"
    },
    "mark": {
        "type": "bar",
        "tooltip": true
    },
    "encoding": {
        "x": {
            "field": "Final_Table_Made",
            "type": "quantitative",
            "bin": true
        },
        "y": {
            "aggregate": "count",
            "type": "quantitative"
        }
    }
}