{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "description": "A histogram showing the number of players selected and not selected.",
    "data": {
        "url": "spider_csv/soccer_2_tryout.csv.csv"
    },
    "mark": {
        "type": "bar",
        "tooltip": true
    },
    "encoding": {
        "x": {
            "field": "decision",
            "type": "nominal"
        },
        "y": {
            "aggregate": "count",
            "type": "quantitative"
        }
    }
}