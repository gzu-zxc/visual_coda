{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "description": "A heatmap of department heads' age distribution by state of birth.",
    "data": {
        "url": "spider_csv/department_management_head.csv.csv"
    },
    "mark": {
        "type": "rect",
        "tooltip": true
    },
    "encoding": {
        "x": {
            "field": "born_state",
            "type": "nominal"
        },
        "y": {
            "field": "age",
            "type": "quantitative",
            "bin": true
        },
        "color": {
            "aggregate": "count",
            "type": "quantitative"
        }
    }
}