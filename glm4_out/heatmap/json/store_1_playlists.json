{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "description": "A heatmap of playlist names vs unique identifiers from Store 1.",
    "data": {
        "url": "spider_csv/store_1_playlists.csv.csv"
    },
    "mark": {
        "type": "rect",
        "tooltip": true
    },
    "encoding": {
        "x": {
            "field": "id",
            "type": "ordinal"
        },
        "y": {
            "field": "name",
            "type": "ordinal"
        },
        "color": {
            "aggregate": "count",
            "type": "quantitative"
        }
    }
}