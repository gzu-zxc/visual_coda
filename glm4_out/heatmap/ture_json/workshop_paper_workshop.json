{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "description": "A heatmap showing the number of workshops per month and venue.",
    "data": {
        "url": "spider_csv/workshop_paper_workshop.csv.csv"
    },
    "mark": {
        "type": "rect",
        "tooltip": true
    },
    "encoding": {
        "x": {
            "field": "Date",
            "type": "temporal",
            "timeUnit": "month",
            "axis": {
                "title": "Month"
            }
        },
        "y": {
            "field": "Venue",
            "type": "ordinal",
            "axis": {
                "title": "Venue"
            }
        },
        "color": {
            "aggregate": "count",
            "type": "quantitative",
            "scale": {
                "scheme": "greenblue"
            }
        }
    }
}